{
  "name": "rac-web-admin",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ci:qa": "ng build --configuration=qa --aot --no-progress",
    "build:ci:prod": "ng build --prod --aot --no-progress",
    "precommit": "lint-staged && yarn test",
    "test": "jest --updateSnapshot",
    "test:coverage": "jest --runInBand --coverage",
    "test:snapshot": "jest --coverage -u",
    "test:watch": "jest --clearCache && jest --watch",
    "lint": "ng lint",
    "lint:watch": "chokidar src/**/*.ts -c \"ng lint\" --initial --verbose",
    "e2e": "ng e2e",
    "beautify": "html-beautify -r src/**/*.html",
    "format": "prettier --config .prettierrc --write {src,e2e}/**/*"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "^0.6.8",
    "@angular-devkit/schematics": "^0.6.8",
    "@angular/animations": "^7.1.2",
    "@angular/cdk": "^6.3.3",
    "@angular/common": "^7.1.2",
    "@angular/compiler": "^7.1.2",
    "@angular/core": "^7.1.2",
    "@angular/forms": "^7.1.2",
    "@angular/http": "^7.1.2",
    "@angular/material": "^6.4.0",
    "@angular/platform-browser": "^7.1.2",
    "@angular/platform-browser-dynamic": "^7.1.2",
    "@angular/router": "^7.1.2",
    "@angular/service-worker": "^7.1.2",
    "@material/animation": "^0.39.0",
    "@ngrx/effects": "^6.0.1",
    "@ngrx/entity": "^6.0.1",
    "@ngrx/router-store": "^6.0.1",
    "@ngrx/schematics": "^6.0.1",
    "@ngrx/store": "^6.0.1",
    "@ngrx/store-devtools": "^6.0.1",
    "@ngui/map": "^0.30.3",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "ng2-cookies": "^1.0.12",
    "ngrx-forms": "^3.0.3",
    "ngrx-store-freeze": "^0.2.4",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.2",
    "@angular/compiler-cli": "^7.1.2",
    "@angular/language-service": "^7.1.2",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.12.17",
    "angular2-template-loader": "^0.6.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "chokidar": "^2.0.4",
    "chokidar-cli": "^1.2.1",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.6.1",
    "husky": "^0.14.3",
    "jest": "^23.5.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "^6.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "js-beautify": "^1.8.4",
    "json-server": "^0.14.0",
    "lint-staged": "^7.2.2",
    "ngx-wallaby-jest": "^0.0.1",
    "prettier": "1.14.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "3.1.6",
    "wallaby-webpack": "^3.9.11"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "tslint --fix",
      "git add"
    ],
    "**/*.{js,ts,scss,md}": [
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "**/*.html": [
      "html-beautify -r",
      "git add"
    ]
  }
}
